    <groupId>com.pinnacle.cart.automation</groupId>
    <artifactId>Pinnacle</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <name>Pinnacle</name>
    <url>http://maven.apache.org</url>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <encoding>UTF-8</encoding>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    
    <dependencies>

